  "license": "MIT",
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.1",
    "bcryptjs": "^2.4.3",
    "concurrently": "^7.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "fs-extra": "^10.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.3.1",
    "multer": "^1.4.4",
    "ndb": "^1.1.5",
    "nodemailer": "^6.7.5",
    "papaparse": "^5.3.2",
    "redux-persist": "^6.0.0",
    "sharp": "^0.30.5",
    "uuid": "^8.3.2"
  },
