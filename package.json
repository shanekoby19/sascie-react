{
  "name": "sassie-app",
  "version": "1.0.0",
  "description": "A web-application to support Acelero and Shine Sassie.",
  "main": "server.js",
  "scripts": {
    "dev:server": "nodemon server.js NODE_ENV=production",
    "dev": "concurrently \"npm run dev:server\" \"npm run start:client\"",
    "debug:dev": "ndb server.js NODE_ENV=development",
    "debug:prod": "ndb server.js",
    "start": "concurrently \"npm run server\" \"npm run start:client\"",
    "server": "node server.js NODE_ENV=production",
    "start:client": "cd sascie-react && npm start",
    "heroku-postbuild": "npm run install:client && run build:client",
    "build:client": "cd sascie-react && npm run build",
    "install:client": "cd sascie-react && npm install"
  },
  "author": "Shane Kobylecky",
  "license": "MIT",
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "concurrently": "^7.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "fs-extra": "^10.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.3.1",
    "multer": "^1.4.4",
    "ndb": "^0.2.4",
    "nodemailer": "^6.7.5",
    "nodemon": "^2.0.19",
    "papaparse": "^5.3.2",
    "redux-persist": "^6.0.0",
    "sharp": "^0.30.5",
    "uuid": "^8.3.2"
  },
  "engines": {
    "node": "16.4.0",
    "npm": "7.18.1"
  }
}
